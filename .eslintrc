{
  "rules": {
    "strict": [
      2,
      "global"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "max-len": [
      2,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": false
      }
    ],
    "no-fallthrough": 0,
    "default-case": 0,
    "no-plusplus": 0,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "no-dupe-keys": 2,
    "no-extra-boolean-cast": 2,
    "no-irregular-whitespace": 2,
    "no-multi-spaces": 2,
    "no-else-return": 0,
    "no-eval": 2,
    "no-multi-str": 2,
    "no-self-compare": 2,
    "no-useless-call": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-use-before-define": 2,
    "radix": 2,
    "curly": 2
  },
  "env": {
    "browser": true,
    "node": true
  }
}
